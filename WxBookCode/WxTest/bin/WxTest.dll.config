<configuration>
  <appSettings>
    <!-- MongoDB数据库链接 -->
    <add key="MongoDBConn" value="mongodb://127.0.0.1:27017"/>
    <!-- MongoDB数据库名称 -->
    <add key="MongoDBName" value="WeChatDev"/>
  </appSettings>
  <system.web>
    <compilation debug="true" targetFramework="4.5" />
    <httpRuntime targetFramework="4.5" />
  </system.web>
</configuration>